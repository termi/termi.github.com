<SCRIPT>
//[attributes] 1
var getAttributes = window._ielt8_getAttributes;

/*@requared window.Node.prototype = { ielt8 : true }*/
var Element_proto=window.Node.prototype,
	getChildren=Element_proto.getchildren,
	getClassList=Element_proto.getclassList,
	getLastElementChild=Element_proto.getlastElementChild,
	getFirstElementChild=Element_proto.getfirstElementChild,
	getNextElementSibling=Element_proto.getnextElementSibling,
	getPreviousElementSibling=Element_proto.getpreviousElementSibling,
	getChildElementCount=Element_proto.getchildElementCount,
	getDataset=Element_proto.getdataset,
	getDPD=function(){return 0x01},
	getDPP=function(){return 0x02},
	getDPF=function(){return 0x04},
	getDPC=function(){return 0x08},
	getDPCB=function(){return 0x10},
	getControl=Element_proto.getcontrol,
	getLabels=Element_proto.getlabels;
	
if(this != window) {//Strange IE bug where this === window
	if(!this.compareDocumentPosition)this.compareDocumentPosition=Element_proto.compareDocumentPosition;
	if(!this.compareDocumentPosition)window.__ielt8__wontfix.push(this);//'OBJECT' tag
	if(!this.getElementsByClassName)this.getElementsByClassName=Element_proto.getElementsByClassName;
	if(!this.insertAfter)this.insertAfter=Element_proto.insertAfter;
	/*@requared: window.addEventListener, window.removeEventListener, window.dispatchEvent */
	if(!this.addEventListener)this.addEventListener=window.addEventListener;
	if(!this.removeEventListener)this.removeEventListener=window.removeEventListener;
	if(!this.dispatchEvent)this.dispatchEvent=window.dispatchEvent;
	if(this.cloneNode!==Element_proto.cloneNode)this.cloneNode=Element_proto.cloneNode;
	if(!this.hasAttribute)this.hasAttribute!==Element_proto.hasAttribute;

	if(!this.querySelectorAll)this.querySelectorAll=window._ielt8_querySelectorAll;
	if(!this.querySelector)this.querySelector=window._ielt8_querySelector;

	if(!this._)this._={};

	//[attributes] 2
	/*
		[BUG]strange behavior on 'object' element: IE < 8 won't create '_' container
		*/
	/*@requared window._ielt8_getAttributes */
	//Save original attributes property if we don't do it
	if(!this._.__ielt8_attributes__)this._.__ielt8_attributes__=this.attributes;
}


</SCRIPT>
